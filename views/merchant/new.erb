
<% x = ["/images/logos/abstract.jpeg"] %>



<form method= "post" action="/merchants/image" enctype="multipart/form-data">
<input type="file" name="file" accept="image/*">
<input type="submit" value="upload logo">
</form>

<form method="post" action="/merchants">
  <label for="name">Name:</label>
  <input id="name" type ="text" name="name" required = "required"/>

  <label for="logo-select">Company Logo:</label>
  <select id="logo-select" name="logo">
    <datalist id="defaultURLs">
      <option value="/images/logos/abstract.jpeg">default</option>
      <option value="/images/logos/shield.png">shield</option>
      <option value="/images/logos/pub.jpeg">pub</option>
      <option value="/images/logos/amazon.png">amazon</option>
      <option value="/images/logos/tesco.png">tesco</option>
</datalist>
  </select>



  <input type="submit" value="Create New Merchant" />
</form>


